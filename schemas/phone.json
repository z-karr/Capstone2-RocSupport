{
    "$id": "phone.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Phone",
    "type": "object",
    "properties": {
        "country_code": {
            "type": "string",
            "maxLength": 5
        },
        "area_code": {
            "type": "string",
            "maxLength": 10
        },
        "phone_number": {
            "type": "string",
            "maxLength": 20
        },
        "phone_type": {
            "type": "string",
            "maxLength": 20
        }
    },
    "required": [
        "country_code",
        "phone_number",
        "phone_type"
    ]
}