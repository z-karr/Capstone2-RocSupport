{
    
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "PatientProfileUpdate",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 100
        },
        "email": {
            "type": "string",
            "format": "email",
            "maxLength": 100
        },
        "password": {
            "type": "string",
            "minLength": 6,
            "maxLength": 100
        },
        "address": {
            "$ref": "./address.json"
        },
        "phone": {
            "$ref": "./phone.json"
        }
    },
    "required": []
}